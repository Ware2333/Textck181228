<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper
	namespace="com.ck.ck181228.customer_info.mapper.Customer_infoMapper">

	<!-- Result Map -->
	<resultMap id="BaseResultMap"
		type="com.ck.ck181228.customer_info.model.Customer_infoModel">
		<result column="id" property="id" />
		<result column="cust_code" property="cust_code" />
		<result column="cust_name" property="cust_name" />
		<result column="status" property="status" />
		<result column="email" property="email" />
	</resultMap>
	<!-- table all fields sql片段 -->
	<sql id="Base_Column_List">
		id,cust_code,cust_name,status,email
	</sql>
	<sql id="Example_Where_Clause">
		where 1=1
		<if test="id != null">
			and id = #{id}
		</if>
		<if test="cust_code != null and cust_code != ''">
			and cust_code = #{cust_code}
		</if>
		<if test="cust_name !=null and cust_name !=''">
			and cust_name = #{cust_name}
		</if>
		<if test="status != null and status != ''">
			and status = #{status}
		</if>
		<if test="email !=null and email !=''">
			and email = #{email}
		</if>
	</sql>
	<insert id="insert" parameterType="Object">
		insert into
		mis.customer_info(cust_code,cust_name,status,email)
		values(#{cust_code},#{cust_name},#{status},#{email})
	</insert>
	<update id="update" parameterType="Object">
		update mis.customer_info set
		id = id
		<if test="cust_code != null and cust_code != ''">
			,cust_code = #{cust_code}
		</if>
		<if test="cust_name !=null and cust_name !=''">
			,cust_name = #{cust_name}
		</if>
		<if test="status != null and status != ''">
			,status = #{status}
		</if>
		<if test="email !=null and email !=''">
			,email = #{email}
		</if>
		where id = #{id}
	</update>
	<update id="updateModel" parameterType="Object">
		update mis.customer_info set
		id = id
		<if test="cust_name !=null and cust_name !=''">
			,cust_name = #{cust_name}
		</if>
		<if test="status != null and status != ''">
			,status = #{status}
		</if>
		<if test="email !=null and email !=''">
			,email = #{email}
		</if>
		where cust_code = #{cust_code}
	</update>
	<delete id="delete" parameterType="Object">
		delete from
		mis.customer_info
		where id = #{id}
	</delete>
	<delete id="deleteModel" parameterType="Object">
		delete from
		mis.customer_info where cust_code = #{cust_code}
	</delete>
	<!-- <select id="selectId" resultMap="BaseResultMap" -->
	<!-- parameterType="Object"> -->
	<!-- select id from mis.user_management where -->
	<!-- user_code=#{user_code} -->
	<!-- </select> -->
	<select id="select" resultMap="BaseResultMap"
		parameterType="Object">
		select
		<include refid="Base_Column_List" />
		from mis.customer_info
		<include refid="Example_Where_Clause" />
	</select>
	<select id="selectList" resultMap="BaseResultMap"
		parameterType="Object">
		select
		<include refid="Base_Column_List" />
		from mis.customer_info
		<include refid="Example_Where_Clause" />
		limit ${rowStart},${pageLimit}
	</select>
	<select id="selectModel" resultMap="BaseResultMap"
		parameterType="Object">
		select
		<include refid="Base_Column_List" />
		from mis.customer_info
		<include refid="Example_Where_Clause" />
	</select>
	<select id="selectCount" resultType="int" parameterType="Object">
		select count(1) from mis.customer_info
		<include refid="Example_Where_Clause"></include>
	</select>
</mapper>